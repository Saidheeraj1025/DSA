def reverse_string(s):
    left, right = 0, len(s) - 1
    while left < right:
        # Swap the characters at left and right
        s[left], s[right] = s[right], s[left]
        # Move the pointers
        left += 1
        right -= 1
chars = ['h', 'e', 'l', 'l', 'o']
reverse_string(chars)
print(chars)                              # Output: ['o', 'l', 'l', 'e', 'h']
